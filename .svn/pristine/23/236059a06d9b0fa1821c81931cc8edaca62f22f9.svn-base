		<class_name>server.xzy.jetty.JettyServer</class_name>
		<create_method></create_method>
		<enable_server>true</enable_server>
		<failure_system_exit>true</failure_system_exit>

		<para key="jetty" value="./jetty/jetty.xml"/>
	</server>
</start>