<?xml version="1.0"  encoding="GBK"?>
<start>
	<server>
		<server_name>logServer</server_name>
		<desc></desc>
		<class_name>com.xzy.base.server.log.LogRecordServer</class_name>
		<create_method>getSingleInstance</create_method>
		<enable_server>true</enable_server>
