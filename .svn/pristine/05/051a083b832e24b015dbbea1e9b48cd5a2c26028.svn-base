package sl.dao.impl;

import java.util.List;

import org.hibernate.Session;

import sl.dao.TLoginDao;
import sl.domain.User;
import sl.util.HibernateUtil;

/**
 * 
 * @author moyer
 * @date 2018Äê1ÔÂ19ÈÕ
